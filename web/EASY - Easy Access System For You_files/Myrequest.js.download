function LoadMyrequestdivDataAgain() {
   
    var ul = document.getElementById("ulmyrequest")
    var txt = document.getElementById("hdnUlmyrequesttext")
    if (txt.value == "") {
        txt.value = ul.innerHTML;
    }
    else {
        $('#ulmyrequest').html(txt.value);
    }
}


